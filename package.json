{
  "name": "@daotl/web-monorepo-starter",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@7.16.0",
  "description": "Web monorepo starter template",
  "repository": {
    "type": "git",
    "url": "git@github.com:daotl/web-monorepo-starter.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "npx eslint . && turbo run lint",
    "lint:fix": "npx eslint --fix . && turbo run lint:fix",
    "dev": "turbo run dev --parallel",
    "pulumi:init": "cd k8s && pnpm i --filter k8s",
    "postinstall": "manypkg check && patch-package",
    "cz": "npx cz"
  },
  "prettier": "@daotl/prettier-config",
  "config": {
    "commitizen": {
      "path": "commitiquette"
    }
  },
  "dependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@daotl/eslint-config": "^0.3.32",
    "@daotl/prettier-config": "^0.1.3",
    "@daotl/tsconfig": "^0.1.2",
    "@manypkg/cli": "^0.19.2",
    "@milahu/patch-package": "^6.4.14",
    "commitiquette": "^1.2.1",
    "commitizen": "^4.2.5",
    "commitlint-config-pnpm-workspace": "^0.2.2",
    "commitlint-plugin-scope-enhanced": "^0.1.0",
    "eslint": "^8.28.0",
    "lefthook": "^1.2.2",
    "prettier": "^2.8.0",
    "rambdax": "^9.0.0",
    "turbo": "^1.6.3",
    "typescript": "4.9.3"
  },
  "volta": {
    "node": "18.9.0"
  }
}
